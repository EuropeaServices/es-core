<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
    <file source-language="en" datatype="plaintext" original="">
        <body>
            <trans-unit id="username">
                <source>username</source>
                <target>Username</target>
            </trans-unit>
           <trans-unit id="password">
                <source>password</source>
                <target>Password</target>
            </trans-unit>
            <trans-unit id="remember_me">
                <source>remember_me</source>
                <target>Remember me</target>
            </trans-unit>
            <trans-unit id="sign_in">
                <source>sign_in</source>
                <target>Sign in</target>
            </trans-unit>
            <trans-unit id="forget_password">
                <source>forget_password</source>
                <target>Forget password</target>
            </trans-unit>

            <trans-unit id="username_or_email">
                <source>username_or_email</source>
                <target>Username or email</target>
            </trans-unit>
            <trans-unit id="return">
                <source>return</source>
                <target>Return</target>
            </trans-unit>
            <trans-unit id="request_new_password">
                <source>request_new_password</source>
                <target>Reset your password</target>
            </trans-unit>
            <trans-unit id="describe_forget_password">
                <source>describe_forget_password</source>
                <target>A link will be sent to you by email to reset your password</target>
            </trans-unit>

            <trans-unit id="describe_change_password">
                <source>describe_change_password</source>
                <target>Change your password</target>
            </trans-unit>

            <trans-unit id="describe_forget_password_request_confirm">
                <source>describe_forget_password_request_confirm</source>
                <target><![CDATA[An e-mail has been sent. It contains a link that you will need to click to reset your password.<br /><br />Note: You can only request a new password every 2 hours.<br /><br />If you do not receive an email, check your spam folder or try again.]]></target>
            </trans-unit>

            <trans-unit id="suject_resetting_mail">
                <source>suject_resetting_mail</source>
                <target>EsCore - Reset your password</target>
            </trans-unit>

            <trans-unit id="body_resetting_mail">
                <source>body_resetting_mail</source>
                <target><![CDATA[Hello %lastname% %firstname% !</br></br>To reset your password, please go to the following address : %url% </br></br>To remember, your username is : <b>%username%</b></br></br>Best regards,</br>]]></target>
            </trans-unit>

            <trans-unit id="form_resetting_new_password">
                <source>form_resetting_new_password</source>
                <target>New password</target>
            </trans-unit>
            <trans-unit id="form_resetting_new_password_confirm">
                <source>form_resetting_new_password_confirm</source>
                <target>Repeat the new password</target>
            </trans-unit>
            <trans-unit id="form_resetting_error">
                <source>form_resetting_error</source>
                <target>Passwords must be identical</target>
            </trans-unit>
            <trans-unit id="form_current_password_label">
                <source>form_current_password_label</source>
                <target>Older password</target>
            </trans-unit>
            <trans-unit id="change_password">
                <source>change_password</source>
                <target>Change the password</target>
            </trans-unit>

            <trans-unit id="must_change_password">
                <source>must_change_password</source>
                <target>Your password has expired, you must change it</target>
            </trans-unit>

            <trans-unit id="suject_warning_pasword_expired_mail">
                <source>suject_warning_pasword_expired_mail</source>
                <target>EsCore - Your password will be expired</target>
            </trans-unit>

            <trans-unit id="body_warning_pasword_expired_mail">
                <source>body_warning_pasword_expired_mail</source>
                <target><![CDATA[Hello %lastname% %firstname% !</br></br>Your password on EsCore will expire at %passwordExpiredAt%, go to %urlChangePassword% to change your password.</br></br>Best regards </br></br>]]></target>
            </trans-unit>
        </body>
    </file>
</xliff>
