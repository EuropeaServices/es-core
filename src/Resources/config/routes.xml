<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing
        http://symfony.com/schema/routing/routing-1.0.xsd">
    <route id="es_core_login" path="/login" methods="GET POST">
        <default key="_controller">es_core.controller.security_controller::login</default>
    </route>
    <route id="es_core_logout" path="/logout" methods="GET">
        <default key="_controller">es_core.controller.security_controller::logout</default>
    </route>
</routes>